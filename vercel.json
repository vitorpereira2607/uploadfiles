{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "src/http/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/http/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  